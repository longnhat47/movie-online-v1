    }\n  },\n  components:{\n    CategoryComponent,\n    \n  },\n  computed: {\n    ...mapState('movie', ['movie']),\n\n  },\n  methods: {\n    ...mapActions('category', ['fetchCategory']),\n    ...mapActions('country', ['fetchCountry']),\n    ...mapActions('movie', ['fetchMovie']),\n    ...mapActions('comment', ['fetchComment']),\n    ...mapActions('user', ['fetchUser']),\n    navHandle(n) {\n      this.navbar.forEach((o) => {\n        if (o == n) {\n          o.isActive = true\n        } else {\n          o.isActive = false\n        }\n      });\n      switch (n.name) {\n        case 'Category':\n          this.fetchCategory().then((o) => this.fillter = o);\n          break;\n        case 'Country':\n          this.fetchCountry().then((o) => this.fillter = o);\n          break;\n        case 'Movie':\n          this.fetchMovie().then((o) => this.fillter = o);\n          break;\n        case 'Comment':\n          this.fetchComment().then((o) => this.fillter = o);\n          break;\n        case 'User':\n          this.fetchUser().then((o) => this.fillter = o);\n          break;\n      }\n    }\n  },\n  created() {\n    this.fetchMovie();\n  }\n\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\na {\n  color: #ccc;\n}\n.home {\n  min-height: 90vh;\n  background-color: #1a1a1a;\n  padding: 48px;\n  color: #ccc;\n}\n</style>\n"],"mappings":"AAgBA,SAASA,UAAU,EAAEC,QAAO,QAAS,MAAK;AAC1C,OAAOC,iBAAgB,MAAO,0CAAyC;AACvE,OAAOC,gBAAe,MAAO,yCAAwC;AACrE,OAAOC,gBAAe,MAAO,yCAAwC;AACrE,OAAOC,cAAa,MAAO,uCAAsC;AACjE,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CACN;QAAEC,IAAI,EAAE,OAAO;QAAEC,KAAK,EAAE,MAAM;QAAEC,IAAI,EAAE,QAAQ;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACjE;QAAEH,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE,UAAU;QAAEC,IAAI,EAAE,WAAW;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAC3E;QAAEH,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE,UAAU;QAAEC,IAAI,EAAE,UAAU;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACzE;QAAEH,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE,WAAW;QAAEC,IAAI,EAAE,UAAU;QA