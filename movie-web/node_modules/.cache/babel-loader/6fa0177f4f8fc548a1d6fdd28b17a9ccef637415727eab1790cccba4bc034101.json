 navbar:[\n        {name:'Danh mục', link:'/category', isActive: false},\n        {name:'Quốc gia', link:'/country', isActive: false},\n        {name:'Phim', link:'/movie', isActive: false},\n        {name:'Bình luận', link:'/comment', isActive: false},\n        {name:'Tài khoản', link:'/user', isActive: false},\n      ],\n      fiilter: ''\n    }\n  },\n  computed: {\n    ...mapState('movie', ['movie'])\n  },\n  methods: {\n    ...mapActions('category', ['fetchMovie']),\n    ...mapActions('country', ['fetchMovie']),\n    ...mapActions('movie', ['fetchMovie']),\n    ...mapActions('comment', ['fetchMovie']),\n    ...mapActions('user', ['fetchMovie']),\n    changeActive(n){\n      this.navbar.forEach((o) =>{\n        if(o == n){\n          o.isActive = true\n        }else{\n          o.isActive = false\n        }\n      })\n    }\n  },\n  created() {\n    this.fetchMovie();\n  }\n\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\na {\n  color: #ccc;\n}\n\n.home {\n  min-height: 90vh;\n  background-color: #1a1a1a;\n  padding: 48px;\n  color: #ccc;\n\n  // .show-more-btn{\n  //   font-size: 16px;\n  // }\n  .movie-item {\n    .movie-link {\n      position: relative;\n      display: block;\n      margin: 4px;\n      overflow: hidden;\n\n\n      .movie-thumbnail {\n        transition: all .3s ease-in-out;\n        object-fit: cover;\n      }\n\n      .icon-play {\n        background: #ff9601;\n        height: 50px;\n        width: 50px;\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        margin: -25px 0 0 -25px;\n        border-radius: 50%;\n        opacity: 0;\n        transform: scale(1.5);\n        transition: all .3s ease-in-out;\n      }\n\n      .icon-play::after {\n        content: \"\";\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        border: 15px solid transparent;\n        border-left: 20px solid #fff;\n        margin: -15px 0 0 -6px;\n      }\n\n      .movie-name {\n        position: absolute;\n        top: 110px;\n        left: 0;\n        padding: 8px;\n        background-color: rgba($color: #000000, $alpha: 0.7);\n        min-height: 28px;\n        font-size: 12px;\n        width: 100%;\n      }\n    }\n  }\n\n  .movie-item:hover .movie-thumbnail {\n    transform: scale(1.2);\n  }\n\n  .movie-item:hover .icon-play {\n    transform: scale(1);\n    opacity: .8;\n  }\n\n\n\n\n  .movie-item-main.movie-item .movie-name {\n    top: 257px;\n\n    font-size: 18px;\n\n  }\n\n\n  .main-show {\n    .movie-item-main.movie-item {\n      .movie-link {\n        height: 300px;\n\n        .movie-thumbnail {\n          height: 100%;\n          width: 100%;\n        }\n      }\n\n    }\n\n    .movie-item {\n      .movie-link {\n        height: 144px;\n\n        .movie-thumbnail {\n          height: 100%;\n          width: 100%;\n        }\n      }\n    }\n  }\n\n  .sub-show {\n    .movie-item {\n      .movie-link {\n        height: 220px;\n\n        .movie-thumbnail {\n          height: 100%;\n          width: 100%;\n        }\n\n        .movie-name {\n          top: 180px;\n          font-size: 16px;\n\n        }\n      }\n    }\n  }\n\n\n\n}</style>\n"],"mappings":"AAeA,SAASA,UAAU,EAAEC,QAAO,QAAS,MAAK;AAC1C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAC,CACL;QAACC,IAAI,EAAC,UAAU;QAAEC,IAAI,EAAC,WAAW;QAAEC,QAAQ,EAAE;MAAK,CAAC,EACpD;QAACF,IAAI,EAAC,UAAU;QAAEC,IAAI,EAAC,UAAU;QAAEC,QAAQ,EAAE;MAAK,CAAC,EACnD;QAACF,IAAI,EAAC,MAAM;QAAEC,IAAI,EAAC,QAAQ;QAAEC,QAAQ,EAAE;MAAK,CAAC,EAC7C;QAACF,IAAI,EAAC,WAAW;QAAEC,IAAI,EAAC,UAAU;QAAEC,QAAQ,EAAE;MAAK,CAAC,EACpD;QAACF,IAAI,EAAC,WAAW;QAAEC,IAAI,EAAC,OAAO;QAAEC,QAAQ,EAAE;MAAK,CAAC,CAClD;MACDC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGP,QAAQ,CAAC,OAAO,EAAE,CAAC,OAAO,CAAC;EAChC,CAAC;EACDQ,OAAO,EAAE;IACP,GAAGT,UAAU,CAAC,UAAU,EAAE,CAAC,YAAY,CAAC,CAAC;IACzC,GAAGA,UAAU,CAAC,SAAS,EAAE,CAAC,YAAY,CAAC,CAAC;IACxC,GAAGA,UAAU,CAAC,OAAO,EAAE,CAAC,YAAY,CAAC,CAAC;IACtC,GAAGA,UAAU,CAA