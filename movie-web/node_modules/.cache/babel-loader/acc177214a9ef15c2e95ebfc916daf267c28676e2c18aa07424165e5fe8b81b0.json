.dataModel.thumbnail = data.thumbnail;\n      this.dataModel.video = data.video;\n      this.dataModel.status = data.status;\n\n      this.isShow = !this.isShow;\n      this.button.edit = !this.button.edit;\n      const el = this.$refs.edit\n      el.value = data.name\n    },\n    deleteModal(data) {\n      this.dataModel.id = data.id;\n      this.isShow = !this.isShow;\n      this.button.delete = !this.button.delete;\n      const el = this.$refs.titleDelete\n      el.innerHTML = 'Bạn có muốn xóa quốc gia ' + `<h1>${data.name}</h1>`\n    },\n    addModel() {\n      this.createMovie(this.dataModel)\n      this.isShow = !this.isShow;\n      this.button.add = !this.button.add;\n    },\n    editModel() {\n      console.log(this.dataModel)\n      // this.updateMovie(this.dataModel)\n      var i = 0;\n      while (i < this.data.length) {\n        if (this.data[i].id === this.dataModel.id) {\n    /* eslint-disable */\n          for(const [key, value] of Object.entries(this.data[i])){\n            // console.log(key)\n            // console.log(value)\n            console.log(this.data[i][key])\n            this.data[i][key] = this.dataModel[key];\n          }\n          console.log(this.data[i])\n          console.log(this.dataModel)\n          break;\n        } else {\n          ++i;\n        }\n      }\n      this.isShow = !this.isShow;\n      this.button.edit = !this.button.edit;\n    },\n    removeModel() {\n      this.deleteMovie(this.dataModel)\n      var i = 0;\n      while (i < this.data.length) {\n        if (this.data[i].id === this.dataModel.id) {\n          console.log(this.data[i]);\n          this.data.splice(i, 1);\n          break;\n        } else {\n          ++i;\n        }\n      }\n      this.isShow = !this.isShow;\n      this.button.delete = !this.button.delete;\n    },\n    getThumbnail(){\n      this.dataModel.thumbnail = this.$refs.thumbnailFile.files[0]\n    },\n    getVideo(){\n      this.dataModel.video = this.$refs.videoFile.files[0]\n      cons